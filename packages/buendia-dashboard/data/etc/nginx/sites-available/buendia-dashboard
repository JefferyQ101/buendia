server {
    listen 80;
    listen 9999;
    server_name _;
    root /usr/share/buendia/dashboard;
    index index.html;

    location /status {
        fastcgi_pass unix:/var/run/fcgiwrap.socket;
        fastcgi_param SCRIPT_NAME status;
        fastcgi_param SCRIPT_FILENAME /usr/share/buendia/dashboard/status;
    }
}
