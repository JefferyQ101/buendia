# This file is written to be included by Makefiles in buendia-* subdirectories
# and assumes that the current directory is one level down.

export PACKAGE_NAME := $(shell basename $$(pwd))
export TOP := $(shell pwd)/../..
export TOOLS := $(TOP)/tools
export FILES := $(shell find root)
PACKAGE=$(PACKAGE_NAME)_$(PACKAGE_VERSION)_all.deb

all: $(PACKAGE)
